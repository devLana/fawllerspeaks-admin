{
  "name": "fawllerspeaks-app",
  "version": "1.0.0",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts,.js",
    "test": "SET NODE_ENV=test&& jest --watch --runInBand",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev:compile": "npm run codegen && npm run dev:build",
    "dev:build": "SET NODE_ENV=development&& ts-node-dev -r tsconfig-paths/register src/index.ts",
    "dev": "npm run dev:build",
    "build:compile": "SET NODE_ENV=demo&& tsc -p tsconfig.build.json",
    "build": "npm run build:compile && tsc-alias -p tsconfig.build.json",
    "start": "node build/index.js"
  },
  "license": "MIT",
  "engines": {
    "node": "20.10.0",
    "npm": "10.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@graphql-codegen/cli": "2.13.11",
    "@graphql-codegen/typescript": "2.8.1",
    "@graphql-codegen/typescript-resolvers": "2.7.6",
    "@jest/globals": "^29.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/formidable": "^3.4.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.17.18",
    "@types/pg": "^8.6.5",
    "@types/sanitize-html": "^2.11.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.57.0",
    "form-data": "^4.0.0",
    "jest": "^29.5.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@apollo/server": "^4.1.0",
    "@supabase/supabase-js": "^2.33.1",
    "@types/nodemailer": "^7.0.2",
    "bcrypt": "^5.1.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "formidable": "^3.5.1",
    "graphql": "^16.6.0",
    "helmet": "^6.0.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^7.0.9",
    "pg": "^8.8.0",
    "sanitize-html": "^2.13.0"
  }
